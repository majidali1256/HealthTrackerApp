<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="3dp"
        android:left="3dp">
        <shape android:shape="rectangle">
            <solid android:color="#180891B2" />
            <corners android:radius="20dp" />
        </shape>
    </item>
    <item
        android:bottom="3dp"
        android:right="3dp">
        <shape android:shape="rectangle">
            <gradient
                android:type="linear"
                android:angle="135"
                android:startColor="#22D3EE"
                android:endColor="#0891B2" />
            <corners android:radius="20dp" />
        </shape>
    </item>
    <item
        android:bottom="4dp"
        android:right="4dp"
        android:top="1dp"
        android:left="1dp">
        <shape android:shape="rectangle">
            <gradient
                android:type="linear"
                android:angle="135"
                android:startColor="#67E8F9"
                android:endColor="#22D3EE" />
            <corners android:radius="18dp" />
        </shape>
    </item>
</layer-list>
